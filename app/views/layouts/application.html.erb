<!DOCTYPE html>
<html>
<head>
  <title>TangoLyricsDB</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div class="container">
		<div class="row">
		      <div class="span9"><%= yield %></div>
		      <div class="span3">
		        <%= render 'shared/search' %>
						<%= render 'shared/stats' %>
		      </div>
		    </div>
	</div>
	
</body>
</html>
